ylQ","executionInfo":{"status":"ok","timestamp":1715183601107,"user_tz":-330,"elapsed":3,"user":{"displayName":"Pk Rai","userId":"18348272567043315595"}},"outputId":"4f221dd4-b39b-4b06-84b0-e0a50bedd3f0"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["(0.39395, 0.03034999999999996)"]},"metadata":{},"execution_count":79}]},{"cell_type":"code","source":["df[(df.atemp<lower_bound)|(df.atemp>upper_bound)]\n"],"metadata":{"id":"bvnYX4K3s49x","executionInfo":{"status":"ok","timestamp":1715183646168,"user_tz":-330,"elapsed":979,"user":{"displayName":"Pk Rai","userId":"18348272567043315595"}},"colab":{"base_uri":"https://localhost:8080/","height":1000},"outputId":"fed3f161-1df5-4261-c856-68534e4bfc7d"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     instant     dteday  season  yr  mnth  hr  holiday  weekday  weathersit  \\\n","12        13 2011-01-01       1   0     1  12        0        6           1   \n","13        14 2011-01-01       1   0     1  13        0        6           2   \n","14        15 2011-01-01       1   0     1  14        0        6           2   \n","15        16 2011-01-01       1   0     1  15        0        6           2   \n","16        17 2011-01-01       1   0     1  16        0        6           2   \n","18        19 2011-01-01       1   0     1  18        0        6           3   \n","19        20 2011-01-01       1   0     1  19        0        6           3   \n","20        21 2011-01-01       1   0     1  20        0        6           2   \n","21        22 2011-01-01       1   0     1  21        0        6           2   \n","22        23 2011-01-01       1   0     1  22        0        6           2   \n","24        25 2011-02-01       1   0     1   0        0        0           2   \n","25        26 2011-02-01       1   0     1   1        0        0           2   \n","26        27 2011-02-01       1   0     1   2        0        0           2   \n","27