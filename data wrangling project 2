4j","executionInfo":{"status":"ok","timestamp":1715182854412,"user_tz":-330,"elapsed":24,"user":{"displayName":"Pk Rai","userId":"18348272567043315595"}},"outputId":"ab6c9ace-ca65-4f55-a1c6-e9aa3d446aa1"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["instant       0\n","dteday        0\n","season        0\n","yr            0\n","mnth          0\n","hr            0\n","holiday       0\n","weekday       0\n","weathersit    0\n","temp          0\n","atemp         0\n","hum           0\n","windspeed     0\n","casual        0\n","registered    0\n","cnt           0\n","dtype: int64"]},"metadata":{},"execution_count":44}]},{"cell_type":"code","source":["dataset_3.isnull().sum()[dataset_3.isnull().sum()>0]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"fS4GDR6q6mMT","executionInfo":{"status":"ok","timestamp":1715182854412,"user_tz":-330,"elapsed":22,"user":{"displayName":"Pk Rai","userId":"18348272567043315595"}},"outputId":"747ee3da-17b6-4d06-ccc9-d2ea7a41d545"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["Series([], dtype: int64)"]},"metadata":{},"execution_count":45}]},{"cell_type":"code","source":["# concatination of the dataset_4 and datase_3 and saved in variable df\n","df=pd.concat([dataset_4,dataset_3],axis=0)"],"metadata":{"id":"v00qGqhe6tuF"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# new data after concatination\n","df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"EvOBqCoC7al9","executionInfo":{"status":"ok","timestamp":1715182854413,"user_tz":-330,"elapsed":20,"user":{"displayName":"Pk Rai","userId":"18348272567043315595"}},"outputId":"5102cf0b-0d29-47e0-b14d-d435a09064e2"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     instant      dteday  season  yr  mnth  hr  holiday  weekday  weathersit  \\\n","0          1  01-01-2011       1   0     1   0    Fa